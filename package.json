{
  "name": "game-sudoku",
  "version": "0.2.0",
  "description": "Solve easy and medium sudokus",
  "main": "dist/index.js",
  "dependencies": {},
  "devDependencies": {
    "@babel/parser": "^7.12.5",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.6",
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.5"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'src/tests/**/*.test.ts'",
    "tsc": "tsc",
    "build": "npm-run-all clean lint test tsc",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts --exec npm run dev:start",
    "start": "node ."
  },
  "keywords": [
    "Sudoku",
    "Generator"
  ],
  "author": "ferranblanche",
  "license": "MIT",
  "homepage": "https://github.com/ferranblanche/sudoku",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
